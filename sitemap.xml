        <xhtml:link rel="alternate" hreflang="en" href="{{ site.url }}/pricing.html" />
        <xhtml:link rel="alternate" hreflang="zh-CN" href="{{ site.url }}/zh/pricing.html" />
        <lastmod>{{ site.time | date_to_xmlschema }}</lastmod>
        <changefreq>monthly</changefreq>
        <priority>0.9</priority>
    </url>


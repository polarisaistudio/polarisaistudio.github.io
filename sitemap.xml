        <loc>{{ site.url }}/zh/about.html</loc>
        <xhtml:link rel="alternate" hreflang="en" href="{{ site.url }}/about.html" />
        <xhtml:link rel="alternate" hreflang="zh-CN" href="{{ site.url }}/zh/about.html" />
        <lastmod>{{ site.time | date_to_xmlschema }}</lastmod>
        <changefreq>monthly</changefreq>
        <priority>0.7</priority>
    </url>

    <url>
        <loc>{{ site.url }}/zh/contact.html</loc>
        <xhtml:link rel="alternate" hreflang="en" href="{{ site.url }}/contact.html" />
